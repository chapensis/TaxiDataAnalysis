<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yangchang.TaxiDataAnalysis.dao.RoadInfoDao" >
    <select id="getRoadInfos" resultType="com.yangchang.TaxiDataAnalysis.bean.po.RoadInfoPO">
        SELECT
          roadId, roadName, roadLon, roadLat, roadLength, roadTime
        FROM
          roadinfoall
    </select>
</mapper>